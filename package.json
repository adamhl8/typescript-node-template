{
  "name": "typescript-node-template",
  "version": "0.1.0",
  "type": "module",
  "description": "A boilerplate/template for TypeScript projects in Node with sensible linting and formatting defaults.",
  "repository": "github:adamhl8/typescript-node-template",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "lint": "eslint -f pretty .",
    "start": "ts-node ./src/index.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@swc/core": "^1.3.5",
    "@types/node": "^18.8.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-unicorn": "^44.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "prettier-plugin-pkg": "^0.17.1",
    "prettier-plugin-sh": "^0.12.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
